function rk
    # Vars
    set OBJ $argv[1]
    set preECHO "!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@!@"
    set postECHO "^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^"
    # Post
    if test -e $OBJ
        # Cmd
        rip $OBJ
        clear

        # Post Cmd
        echo $preECHO
        lsd
        echo $postECHO
        echo "RIP>" $OBJ
    else
        echo "FILE/DIR DOES NOT EXIST!"
    end
end
